<aura:component
  implements="force:lightningQuickAction,force:hasRecordId"
  access="global"
>
  <aura:attribute name="record" type="String" />
  <aura:attribute name="recordData" type="Object" />
  <aura:attribute name="autoclose" type="Boolean" default="false" />
  <lightning:workspaceAPI aura:id="workspace" />
  <aura:handler name="init" value="{!this}" action="{!c.doinit}" />
</aura:component>
